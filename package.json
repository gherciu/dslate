{
  "private": true,
  "license": "MIT",
  "homepage": "http://rojer95.github.io/dslate",
  "workspaces": [
    "packages/*"
  ],
  "description": "DSlate组件 - Component of DSlate",
  "keywords": [
    "antd",
    "wysiwyg",
    "rich-editor",
    "ant-design",
    "slate",
    "slate-react",
    "dslate"
  ],
  "gitHooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "scripts": {
    "dev": "pnpm run --filter '@dslate/*' dev",
    "build": "pnpm run --filter '@dslate/*' build",
    "preview": "pnpm run --filter '@dslate/*' preview",
    "docs:dev": "cross-env APP_ROOT=dumi dumi dev",
    "docs:build": "cross-env APP_ROOT=dumi dumi build",
    "docs:deploy": "gh-pages -d ./dumi/dist",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "ci:changeset": "changeset",
    "ci:version": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "cross-env": "^7.0.3",
    "dumi": "^2.2.1",
    "gh-pages": "^5.0.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@douyinfe/semi-ui": "^2.28.1",
    "@dslate/antd": "workspace:2.0.0-beta.6",
    "@dslate/component": "workspace:2.0.0-beta.6",
    "@dslate/core": "workspace:2.0.0-beta.6",
    "@dslate/semi": "workspace:2.0.0-beta.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "antd": "^5.1.7",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slate": "^0.90.0",
    "slate-react": "^0.90.0"
  }
}
