    "slate-react": "^0.90.0"
  },
  "devDependencies": {
    "@types/escape-html": "^1.0.2",
    "@types/node": "^14.18.36",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
