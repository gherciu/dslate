  "keywords": [
    "wysiwyg",
    "rich-editor",
    "ant-design",
    "slate",
    "slate-react",
    "dslate"
  ],
  "homepage": "https://github.com/rojer95/dslate/tree/master/packages/component#readme",
  "bugs": "https://github.com/rojer95/dslate/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/rojer95/dslate"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
